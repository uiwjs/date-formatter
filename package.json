{
  "name": "@uiw/formatter",
  "version": "2.0.2",
  "description": "Get a formatted date.",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "unpkg": "dist/formatter.js",
  "typings": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    },
